{
  "name": "cdp-reverter",
  "version": "0.0.1",
  "description": "Generate a working CDP script from ws connexion",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "clean": "rimraf {test,lib,src,scripts}/*.{d.ts,js,js.map} ./index.{js,d.ts,js.map}",
    "rebuild": "npm run clean && tsc",
    "prepare": "npm run clean && tsc"
  },
  "keywords": [
    "chrome",
    "reverse-engineer",
    "proxy",
    "debug",
    "Chrome DevTools Protocol"
  ],
  "author": "Uriel Chemouni <uchemouni@gmail.com> (https://urielch.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/ws": "^8.5.3",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "picocolors": "^1.0.0",
    "ws": "^8.8.1"
  },
  "bin": {
    "cdp-reverter": "client.js"
  },
  "files": [
    "src/*.js",
    "src/*.d.ts",
    "client.js"
  ]
}
