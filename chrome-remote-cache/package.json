{
  "name": "chrome-remote-cache",
  "version": "0.1.3",
  "description": "Caching system for chrome devtools protocol",
  "keywords": [
    "chrome",
    "cache",
    "chrome devtools protocol"
  ],
  "homepage": "https://github.com/UrielCh/ChromeDevToolsProtocol",
  "repository": {
    "type": "git",
    "url": "git://github.com/UrielCh/ChromeDevToolsProtocol.git"
  },
  "bugs": {
    "url": "http://github.com/UrielCh/ChromeDevToolsProtocol/issues"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "ava",
    "clean": "rimraf {test,lib,src,scripts}/*.{d.ts,js,js.map} ./index.{js,d.ts,js.map}",
    "rebuild": "npm run clean && tsc",
    "prepare": "npm run clean && tsc && ava"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@u4/chrome-remote-interface": "^0.4.8",
    "ioredis": "^5.2.4",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "ava": "3.15.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "ava": {
    "failFast": true,
    "verbose": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/**/*.spec.ts"
    ]
  },
  "files": [
    "src/*.js",
    "src/*.d.ts"
  ]
}